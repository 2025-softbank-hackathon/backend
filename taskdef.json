{
  "executionRoleArn": "arn:aws:iam::137068226866:role/chatapp-dev-ecs-task-execution-role",
  "taskRoleArn": "arn:aws:iam::137068226866:role/chatapp-dev-ecs-task-role",
  "containerDefinitions": [
    {
      "name": "chatapp-container",
      "image": "137068226866.dkr.ecr.ap-northeast-2.amazonaws.com/chatapp-dev:latest",
      "essential": true,
      "portMappings": [
        { "containerPort": 3000, "hostPort": 3000, "protocol": "tcp" }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "chatapp-dev-task-green"
}
